{
  "name": "narraleaf",
  "version": "0.0.1-alpha.3",
  "description": "Create your visual novel with Electron and React",
  "bin": "./dist/cli.cjs",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MPL-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    },
    "./main": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "node project/esbuild.js && tsc --emitDeclarationOnly && tsc-alias",
    "watch": "node project/esbuild.js --watch & tsc --emitDeclarationOnly --watch",
    "prepublishOnly": "yarn run build",
    "postinstall": "npm install --save-dev babel-loader@^9.2.1 electron@^34.2.0 electron-builder@^25.1.8 html-webpack-plugin@^5.6.3 webpack@^5.98.0 chalk@^4.1.2 commander@^13.1.0"
  },
  "packageManager": "yarn@4.4.1",
  "keywords": [
    "react",
    "electron",
    "visual-novel",
    "typescript",
    "narraleaf",
    "nvl"
  ],
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@inquirer/prompts": "^7.3.2",
    "@swc/core": "^1.10.18",
    "@swc/core-win32-x64-msvc": "^1.10.18",
    "@swc/wasm": "^1.10.18",
    "css-loader": "^7.1.2",
    "ejs": "^3.1.10",
    "jsonc": "^2.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "style-loader": "^4.0.0",
    "uglify-js": "^3.19.3",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "babel-loader": "^9.2.1",
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/lodash": "^4.17.15",
    "@types/mime-types": "^2",
    "@types/react": "^19.0.10",
    "@types/webpack-node-externals": "^3",
    "@types/ws": "^8",
    "babel-loader": "^9.2.1",
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "electron": "^34.3.1",
    "electron-builder": "^25.1.8",
    "esbuild": "0.25.0",
    "esbuild-css-modules-plugin": "^3.1.4",
    "html-webpack-plugin": "^5.6.3",
    "motion": "^12.4.10",
    "narraleaf-react": "^0.3.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.0",
    "webpack": "^5.98.0",
    "webpack-node-externals": "^3.0.0"
  },
  "files": [
    "dist"
  ]
}
