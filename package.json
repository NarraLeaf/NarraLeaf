{
  "name": "narraleaf",
  "version": "0.0.1",
  "description": "Create your visual novel with Electron and React",
  "bin": "./dist/cli.js",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "node project/esbuild.js && tsc --emitDeclarationOnly",
    "watch": "node project/esbuild.js --watch & tsc --emitDeclarationOnly --watch",
    "prepublishOnly": "yarn run build"
  },
  "packageManager": "yarn@4.4.1",
  "keywords": [
    "react",
    "electron",
    "visual-novel",
    "typescript",
    "narraleaf",
    "nvl"
  ],
  "devDependencies": {
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/ejs": "^3",
    "@types/lodash": "^4",
    "@types/node": "^22.13.4",
    "esbuild": "0.25.0",
    "raw-loader": "^4.0.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@inquirer/prompts": "^7.3.2",
    "babel-loader": "^9.2.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "css-loader": "^7.1.2",
    "ejs": "^3.1.10",
    "html-webpack-plugin": "^5.6.3",
    "jsonc": "^2.0.0",
    "lodash": "^4.17.21",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0",
    "zod": "^3.24.2"
  }
}
