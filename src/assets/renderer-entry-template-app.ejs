import React from "react";
import { createRoot } from "react-dom/client";
import { render } from "narraleaf/client";

import { default as App, story } from "<%= appPath %>";
import * as NarraLeafReact from "narraleaf-react";

//// Import pages
<% pages.forEach(function(page, index) { %>// <%= page %>
import {default as __NARRALEAF_IMPORTED_PAGE_<%= index %>} from "<%= page %>";<% }); %>
////

const pages = [<% pages.forEach(function(page, index) { %>
    __NARRALEAF_IMPORTED_PAGE_<%= index %><%= index < pages.length - 1 ? ',' : '' %>
<% }); %>];

const root = createRoot(document.getElementById("root"));
!async function () {
    await render(root, {
        NarraLeafReact,
        story,
        App,
        pages,
    });
}();
