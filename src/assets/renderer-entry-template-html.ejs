    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <base href="<%= base %>">
    <meta http-equiv="Content-Security-Policy" content="
        default-src 'self' <%= protocol %>:;
        img-src 'self' <%= protocol %>: data: <%= allowHTTP ? 'http: https:' : '' %>;
        script-src 'self' <%= protocol %>: <%= protocol !== 'app' ? 'app: app://renderer' : 'app://renderer' %> <%- devMode ? "'unsafe-eval'" : '' %>;
        font-src 'self' <%= protocol %>: <%= protocol !== 'app' ? 'app: app://renderer' : 'app://renderer' %>;
        style-src 'self' 'unsafe-inline';
    ">
    <script defer src="<%= script %>"></script>
</head>
<body>
<div id="root"></div>
</body>
</html>
